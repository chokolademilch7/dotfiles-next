# alias
alias co=code-insiders
alias cu=cursor
alias t=tmux
alias g=git
alias d=docker
alias ga="current-gh-auth"
alias gs="gh auth switch"
alias ls=lsd
alias la="lsd -la"
alias ll="lsd -l"
alias lla="lsd -lla"
alias lg="lazygit --use-config-file=$HOME/.config/lazygit/config.yml"
alias lgs="lazygit --use-config-file=$HOME/.config/lazygit/config.yml status"
alias lgl="lazygit --use-config-file=$HOME/.config/lazygit/config.yml log"
alias ldo="lazydocker"
alias ldn="lazynpm"
alias hm=home-manager
alias hme="home-manager edit"
alias hms="home-manager switch --impure"
alias x="xplr"
alias wt="git worktree"
alias sl="mprocs --npm"
alias yolo="claude --dangerously-skip-permissions"
alias win="ssh win"
alias wsl="ssh wsl"
alias wake="wakeonlan $WINDOWS_MAC_ADDRESS"
alias wakewin="ssh -t  pi 'bash -ic wakewin'"
alias gemi="gemini -m gemini-2.5-flash"
alias c='claude --model haiku --dangerously-skip-permissions'
alias cl='claude --model sonnet --dangerously-skip-permissions'
alias serena='claude mcp add serena -- uvx --from git+https://github.com/oraios/serena serena start-mcp-server --enable-web-dashboard false  --context ide-assistant --project "$(pwd)"'
